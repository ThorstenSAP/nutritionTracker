 <mvc:View controllerName="tm.nutriTracker.myUI5App.controller.MainView"
  displayBlock="true"
  xmlns="sap.m"
  xmlns:mvc="sap.ui.core.mvc">
      <Page title="{i18n>title}">
        <content>
          <List
            headerText="Meals"
            items="{
              path: '/Meals'
            }" >
            <StandardListItem
              title="{name}"
              description="{dateEaten}"
              type="Navigation"
              press="displayNutriDetails"/>
          </List>
        </content>
      </Page>
</mvc:View>