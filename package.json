{
  "name": "nutritionTracker",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^5",
    "express": "^4"
  },
  "devDependencies": {
    "@sap/ux-specification": "^1.90.1",
    "sqlite3": "^5.0.2",
    "shx": "^0.3.3",
    "@ui5/cli": "^2.8.1",
    "karma": "^6.0.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-ui5": "^2.3.2",
    "npm-run-all": "^4.1.5",
    "eslint": "^7.18.0",
    "ui5-middleware-livereload": "^0.5.4",
    "ui5-middleware-simpleproxy": "^0.7.2"
  },
  "ui5": {
    "dependencies": [
      "ui5-middleware-livereload",
      "ui5-middleware-simpleproxy"
    ]
  },
  "scripts": {
    "start": "npm-run-all --parallel start-ui5 start-cap",
    "start-cap": "cds watch",
    "start-ui5": "ui5 serve --config=uimodule/ui5.yaml  --open index.html"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es2020": true,
      "node": true,
      "jest": true,
      "mocha": true
    },
    "globals": {
      "SELECT": true,
      "INSERT": true,
      "UPDATE": true,
      "DELETE": true,
      "CREATE": true,
      "DROP": true,
      "CDL": true,
      "CQL": true,
      "CXL": true,
      "cds": true
    },
    "rules": {
      "no-console": "off",
      "require-atomic-updates": "off"
    }
  },
  "sapux": [
    "app/tm.nutritracker",
    "app/project1",
    "app/listreport",
    "app/nutritracker",
    "app/project1"
  ],
  "cds": {
    "requires": {
      "db": {
        "kind": "sqlite",
        "model": "*",
        "credentials": {
          "database": "db/my.db"
        }
      }
    }
  }
}
